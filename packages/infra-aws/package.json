    "@aws-cdk/aws-logs": "1.x",
    "@aws-cdk/aws-route53": "1.x",
    "@aws-cdk/aws-route53-targets": "1.x",
    "@aws-cdk/aws-s3": "1.x",
    "@aws-cdk/aws-s3-assets": "1.x",
    "@aws-cdk/aws-sam": "1.x",
    "@aws-cdk/aws-secretsmanager": "1.x",
    "@aws-cdk/aws-servicediscovery": "1.x",
    "@aws-cdk/aws-sns": "1.x",
    "@aws-cdk/aws-sns-subscriptions": "1.x",
    "@aws-cdk/aws-sqs": "1.x",
    "@aws-cdk/aws-ssm": "1.x",
    "@aws-cdk/aws-stepfunctions": "1.x",
    "@aws-cdk/aws-stepfunctions-tasks": "1.x",
    "@aws-cdk/core": "1.x",
    "@aws-cdk/custom-resources": "1.x",
    "@aws-cdk/cx-api": "1.x",
    "@aws-cdk/region-info": "1.x",
    "@types/fs-extra": "9.x",
    "@typescript-eslint/eslint-plugin": "4.x",
    "@typescript-eslint/parser": "4.x",
    "aws-cdk": "1.x",
    "constructs": "3.x",
    "eslint": "7.x",
    "eslint-config-prettier": "7.x",
    "eslint-plugin-node": "11.x",
    "fs-extra": "9.x",
    "import-sort-style-module": "6.x",
    "jest": "26.x",
    "prettier": "2.x",
    "source-map-support": "0.5.x",
    "ts-jest": "26.x",
    "tsas-cdk": "0.x"
  },
  "config": {
    "appStacks": "EntryServiceStack",
    "deployStacks": "DeployStack",
    "buildStacks": "BuildStack"
  },
  "scripts": {
    "deploy": "cdk deploy --require-approval never $npm_package_config_appStacks",
    "lint": "eslint --cache './**/*.{ts,tsx}'",
    "lint-fix": "yarn run eslint --cache --fix './**/*.{ts,tsx}' && yarn run prettier --write './**/*.{ts,tsx}'",
    "upgrade:show": "ncu",
    "upgrade:do": "ncu -u",
    "params:push": "tsas-cdk param put-all --stage ${STAGE:-dev} && tsas-cdk param put-all --stage ${STAGE:-dev} --division e2e",
    "test:unit": "jest --config=jest.config.unit.js --coverage",
    "diff": "cdk diff $npm_package_config_app-stacks",
    "synth": "cdk synth $npm_package_config_app-stacks",
    "build": "tsc",
    "clean": "shx rm -rf cdk.out/ && shx rm -rf coverage/"
  }
}
