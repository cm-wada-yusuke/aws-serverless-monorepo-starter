    "@aws-cdk/aws-cloudformation": "1.x",
    "@aws-cdk/aws-cloudfront": "1.x",
    "@aws-cdk/aws-cloudwatch": "1.x",
    "@aws-cdk/aws-codebuild": "1.x",
    "@aws-cdk/aws-codecommit": "1.x",
    "@aws-cdk/aws-codedeploy": "1.x",
    "@aws-cdk/aws-codepipeline": "1.x",
    "@aws-cdk/aws-codepipeline-actions": "1.x",
    "@aws-cdk/aws-dynamodb": "1.x",
    "@aws-cdk/aws-ec2": "1.x",
    "@aws-cdk/aws-ecr": "1.x",
    "@aws-cdk/aws-ecr-assets": "1.x",
    "@aws-cdk/aws-ecs": "1.x",
    "@aws-cdk/aws-elasticloadbalancing": "1.x",
    "@aws-cdk/aws-elasticloadbalancingv2": "1.x",
    "@aws-cdk/aws-events": "1.x",
    "@aws-cdk/aws-events-targets": "1.x",
    "@aws-cdk/aws-glue": "1.x",
    "@aws-cdk/aws-iam": "1.x",
    "@aws-cdk/aws-kinesisfirehose": "1.x",
